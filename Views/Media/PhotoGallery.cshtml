@{
    ViewBag.Title = "PhotoGallery";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<eLocal.Gallery> images = ViewBag.images;
    if(images == null){
        images = new List<eLocal.Gallery>();
    }
}
<link href="/Content/media.css" rel="stylesheet" type="text/css" />
<link href="http://fonts.googleapis.com/css?family=Shadows Into Light&subset=latin" rel="stylesheet" type="text/css">
<div id="main"></div>
<div id="thumbs">
    <img src="/Content/img/left_gallery.png" class="arrow_left" alt="View Images to left" />
    <div id="thumb_container">
        <ul>
            @foreach (eLocal.Gallery img in images) {
                //System.Drawing.Image image = System.Drawing.Image.("http://localhost:2808/Content/img/uploaded/" + img.img_path);
                <li>
                    <img src="/Content/img/uploaded/@img.img_path" alt="@img.title" />
                </li>
            }
        </ul>
    </div>
    <img src="/Content/img/right_gallery.png" class="arrow_right" alt="View Images to right" />
</div>
<script src="/Scripts/custom/gallery.js" type="text/javascript"></script>
